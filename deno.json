{
  "lint": {
    "exclude": [
      "dist/"
    ]
  },
  "fmt": {
    "exclude": [
      "dist/"
    ]
  },
  "imports": {
    "std/": "https://deno.land/std@0.211.0/",
    "esbuild/": "https://deno.land/x/esbuild@v0.19.11/",
    "esbuild-gas-plugin": "https://esm.sh/esbuild-gas-plugin@0.8.0/",
    "esbuild_plugin_http_fetch/": "https://deno.land/x/esbuild_plugin_http_fetch@v1.0.3/"
  },
  "tasks": {
    "build": "deno run --allow-read --allow-env --allow-net --allow-write --allow-run ./bin/build.ts",
    "clean": "deno run --allow-read --allow-write ./bin/clean.ts"
  },
  "compilerOptions": {
    "types": [
      "./types/global.d.ts",
      "https://esm.sh/@types/google-apps-script@1.0.78/index.d.ts",
      "https://esm.sh/@types/google-apps-script-oauth2@38.0.4/index.d.ts"
    ]
  }
}
